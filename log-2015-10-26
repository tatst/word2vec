[93m344 [m  if (hs) { /* hs=0„Çà„Çä„Åì„ÅÆifÊñá„ÅØFALSE */
[93m345 [m    a = posix_memalign((void **)&syn1, 128, (long long)vocab_size * layer1_size * sii[24;1H[93m    [mzeof(real)); /* (long long)vocab_size * layer1_size * sizeof(real) bytes„ÅÆ„É°„É¢„É™„ÇíÂâ≤[25;1H[93m    [mÂΩì„Å¶ÔºåÂâ≤ÂΩì„Åü„É°„É¢„É™„ÅÆ„Ç¢„Éâ„É¨„Çπ„Çí(void **)&syn1„Å´Ââ≤ÂΩì„ÇãÔºéÂâ≤ÂΩì„Åü„É°„É¢„É™„ÅÆ„Ç¢„Éâ„É¨„Çπ„ÅØ128„ÅÆ[94m>>[m[26;1H[93m    [mÂÄçÊï∞ */
[93m346 [m    if (syn1 == NULL) {printf("Memory allocation failed\n"); exit(1);} /* ‰∏ä„Åß„É°„É¢„É™[28;1H[93m    [m„ÅÆÂâ≤ÂΩì„Å´Â§±Êïó„Åó„ÅüÊôÇ„Å´Ë°®Á§∫ */[14;5H[?12l[?25h[?25l[27m[m[H[2J[1;1H[93m295 [m  file_size = ftell(fin);

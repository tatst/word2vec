[93m327 [m    printf("Words in train file: %lld\n", train_words);
[93m328 [m  }
[93m329 [m  fin = fopen(train_file, "rb"); /* Ë™≠Âá∫„É¢„Éº„Éâ„Åßtrain_file„ÇíÈñã„Åè */

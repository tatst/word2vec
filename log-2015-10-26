[93m270 [m    printf("ERROR: training data file not found!\n");
[93m271 [m    exit(1);
[93m272 [m  }
[93m273 [m  vocab_size = 0;
[93m274 [m  AddWordToVocab((char *)"</s>"); /* ÂçòË™û„ÇíË™ûÂΩô„Å´Âä†„Åà„ÇãÈñ¢Êï∞AddWordToVocab */

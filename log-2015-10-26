[93m523 [m[9C if (f > MAX_EXP) g = (label - 1) * alpha;
[93m524 [m[9C else if (f < -MAX_EXP) g = (label - 0) * alpha;
[93m525 [m[9C else g = (label - expTable[(int)((f + MAX_EXP) * (EXP_TABLE_SIZE / MAX_EXP  [14;1H[93m    [m/ 2))]) * alpha;

[93m588 [m[9C closev += cent[layer1_size * b + c] * cent[layer1_size * b + c];[25;7H[?12l[?25h[26;7H[27;7H[28;7H[?25l[1;28r[1;1H[2M[1;29r[27;1H[93m589 [m[7C }

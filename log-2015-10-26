[93m382 [m[8C word_count_actual / ((real)(now - start + 1) / (real)CLOCKS_PER_SEC * 1000)))[28;1H[93m    [m;[14;18H[?12l[?25h[?25l[29;1H?fopen[27m[m[H[2J[1;1H[93m317 [m  while (1) { /* ÁÑ°Èôê„É´„Éº„Éó */

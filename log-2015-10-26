545 [mvoid TrainModel() { /* 614è¡Œç›®ã¾ã§ */
[93m546 [m  long a, b, c, d;
[93m547 [m  FILE *fo;
[93m548 [m  pthread_t *pt = (pthread_t *)malloc(num_threads * sizeof(pthread_t)); /* pthread(ãƒ[12;1H[93m    [mãƒ«ãƒã‚¹ãƒ¬ãƒƒãƒ‰ã®ãƒ©ã‚¤ãƒ–ãƒ©ãƒª) */

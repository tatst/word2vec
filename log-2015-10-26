[93m313 [m    exit(1);
[93m314 [m  }
[93m315 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1; /* 0 <= a < vocab_hash_ss[25;1H[93m    [mize„ÅÆÊôÇ */

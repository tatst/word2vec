[1;29r[28;1H[93m592 [m      }[?12l[?25h[?25l[1;28r[28;1H
[1;29r[28;1H[93m593 [m      for (c = 0; c < vocab_size; c++) {[28;7H[?12l[?25h[?25l[1;28r[28;1H

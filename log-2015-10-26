[93m285 [m      a = AddWordToVocab(word);
[93m286 [m      vocab[a].cn = 1;
[93m287 [m    } else vocab[i].cn++;
[93m288 [m    if (vocab_size > vocab_hash_size * 0.7) ReduceVocab();
[93m289 [m  }

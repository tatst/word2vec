[93m600 [m[11C closev = x;
[93m601 [m[11C closeid = d;
[93m602 [m[9C }
[93m603 [m[7C }
[93m604 [m[7C cl[c] = closeid;
[93m605 [m      }
[93m606 [m    }
[93m607 [m    // Save the K-means classes
[93m608 [m    for (a = 0; a < vocab_size; a++) fprintf(fo, "%s %d\n", vocab[a].word, cl[a]);

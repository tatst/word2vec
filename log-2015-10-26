[93m358 [m    syn0[a * layer1_size + b] = (((next_random & 0xFFFF) / (real)65536) - 0.5) / layee[10;1H[93m    [mr1_size;
[93m359 [m  }
[93m360 [m  CreateBinaryTree(); /* 198Ë°åÁõÆ„ÅßÂÆöÁæ©„Åó„ÅüCreateBinaryTree()„ÅßHuffmanÊú®„ÇíÁîüÊàê */
[93m361 [m}
[93m362 

[93m450 [m[9C for (c = 0; c < layer1_size; c++) syn1[c + l2] += g * neu1[c];
[93m451 [m[7C }
[93m452 [m[7C // NEGATIVE SAMPLING
[93m453 [m[7C if (negative > 0) for (d = 0; d < negative + 1; d++) {
[93m454 [m[9C if (d == 0) {
[93m455 [m[11C target = word;
[93m456 [m[11C label = 1;
[93m457 [m[9C } else {

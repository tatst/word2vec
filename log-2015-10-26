[93m546 [m  long a, b, c, d;
[93m547 [m  FILE *fo;
[93m548 [m  pthread_t *pt = (pthread_t *)malloc(num_threads * sizeof(pthread_t)); /* pthread([94m>>[m[11;1H[93m    [m„Éû„É´„ÉÅ„Çπ„É¨„ÉÉ„Éâ„ÅÆ„É©„Ç§„Éñ„É©„É™) */
[93m549 [m  printf("Starting training using file %s\n", train_file);
[93m550 [m  starting_alpha = alpha;
[93m551 [m  if (read_vocab_file[0] != 0) ReadVocab(); else LearnVocabFromTrainFile();
[93m552 [m  if (save_vocab_file[0] != 0) SaveVocab();
[93m553 [m  if (output_file[0] == 0) return;
[93m554 [m  InitNet();
[93m555 [m  if (negative > 0) InitUnigramTable();
[93m556 [m  start = clock();
[93m557 [m  for (a = 0; a < num_threads; a++) pthread_create(&pt[a], NULL, TrainModelThread, (([21;1H[93m    [mvoid *)a); /* 0 <= 1 < num_threads„ÅßÊñ∞Ë¶è„Å´„Çπ„É¨„ÉÉ„Éâ(ID: pt[a])„Çí‰ΩúÊàê„ÅóÔºåTrainModelThrr[22;1H[93m    [mead„ÇíÂÆüË°å„Åô„Çã */
[93m558 [m  for (a = 0; a < num_threads; a++) pthread_join(pt[a], NULL); /* „Çπ„É¨„ÉÉ„ÉâID: pt[a][94m>>[m[24;1H[93m    [m„ÅåÁµÇ‰∫Ü(TrainModelThread„Åå543Ë°åÁõÆ„Åß„Çπ„É¨„ÉÉ„Éâ„ÇíÁµÇ„Çè„Çâ„Åõ„Çã)„Åô„Çã„Åæ„ÅßÂÆüË°å„Çí‰∏ÄÊôÇÂÅúÊ≠¢ */
[93m559 [m  fo = fopen(output_file, "wb"); /* output_file„Çí„Éê„Ç§„Éä„É™„É¢„Éº„Éâ„ÅßÊõ∏ËæºÂ∞ÇÁî®„ÅßÈñã„ÅèÔºå6133[26;1H[93m    [mË°åÁõÆ„ÅßÈñâ„Åò„Çã */
[93m560 [m  if (classes == 0) { /* „Éá„Éï„Ç©„É´„Éà„Åß„ÅØ44Ë°åÁõÆ„ÅÆclasses = 0„Çà„ÇäTRUEÔºå569Ë°åÁõÆ„Åæ„Åß */
[93m561 [m    // Save the word vectors[14;36H[?12l[?25hReadVoca[?25lb[46m()[?12l[?25h[?25l([?12l[?25h[?25l[m()[?12l[?25h; else LearnVocabFromTrainFil[?25le[46m()[?12l[?25h[?25l([?12l[?25h[?25l[m()[?12l[?25h[?25l[29;1H[1m-- INSERT --[14;80H[?12l[?25h[?25l[m;/[?12l[?25h[?25l[14;80H[K[14;80H[?12l[?25h[?25l [?12l[?25h[?25l /[?12l[?25h[?25l/*[?12l[?25h[?25l [?12l[?25h[?25l [?12l[?25h[?25l *[?12l[?25h[?25l*/[?12l[?25h[?25l 3 */[?12l[?25h[?25l30 */[14;86H[?12l[?25h[?25l[15;28r[15;1H[L[1;29r[14;85H06 **[15;1H[93m    [m/[14;87H[?12l[?25h[?25l6Ë°å[15;4H[93m [mÁõÆ */[?12l[?25h[?25l„ÅÆ */[?12l[?25h[?25lR */[?12l[?25h[?25lRe */[?12l[?25h[?25lea */[?12l[?25h[?25lad */[?12l[?25h[?25ldV */[?12l[?25h[?25lVo */[?12l[?25h[?25loc */[?12l[?25h[?25lca */[?12l[?25h[?25lab */[?12l[?25h[?25lb„Å® */[?12l[?25h[?25l2 */[?12l[?25h[?25l26 */[?12l[?25h[?25l63 */[?12l[?25h[?25l3Ë°åÁõÆ */[?12l[?25h[?25l„ÅÆ */[?12l[?25h[?25lL */[?12l[?25h[?25lLe */[?12l[?25h[?25lea */[?12l[?25h[?25lar */[?12l[?25h[?25lrn */[?12l[?25h[?25lnV */[?12l[?25h[?25lVo */[?12l[?25h[?25loc */[?12l[?25h[?25lca */[?12l[?25h[?25lab */[?12l[?25h[?25lbF */[?12l[?25h[?25lFr */[?12l[?25h[?25lro */[?12l[?25h[?25lom */[?12l[?25h[?25lmT */[?12l[?25h[?25lTr */[?12l[?25h[?25lra */[?12l[?25h[?25lai */[?12l[?25h[?25lin */[?12l[?25h[?25lnF */[?12l[?25h[?25lFi */[?12l[?25h[?25lil */[?12l[?25h[?25lle */[?12l[?25h[14;87H[?25l r306[94m>>[m[15;1H[93m    [mË°åÁõÆ„ÅÆReadVocab„Å®263Ë°åÁõÆ„ÅÆLearnVocabFromTrainFile */[14;85H[?12l[?25h[?25lre3066[15;1H[93m [14;86H[?12l[?25h[?25l[mea300[15;1H[93m    [m6Ë
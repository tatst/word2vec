[93m696 [m  expTable = (real *)malloc((EXP_TABLE_SIZE + 1) * sizeof(real)); /* expTable„Å´ÂãïÁöÑ[94m>>[m[27;1H[93m    [m„Å´„É°„É¢„É™Ââ≤ÂΩì(„Å©„Åì„ÅßËß£ÊîæÔºü) */
[93m697 [m  for (i = 0; i < EXP_TABLE_SIZE; i++) {[27;34H[?12l[?25h[?25l[28;45H[?12l[?25h[?25l[1;28r[1;1H[3M[1;29r[26;1H[93m698 [m    expTable[i] = exp((i / (real)EXP_TABLE_SIZE * 2 - 1) * MAX_EXP); // Precompute tt[27;1H[93m    [mhe exp() table
[93m699 [m    expTable[i] = expTable[i] / (expTable[i] + 1);[18C // Precompute f[28;1H[94m@                                                                                       [27;19H[?12l[?25h[?25l[25;45H[?12l[?25h[24;34H[?25l[m[22;36H[46m([12C)[?12l[?25h[?25l[m[22;36H([12C)[20;32H[?12l[?25h[18;50H[20;32H[?25l[22;36H[46m([12C)[?12l[?25h[?25l[m[22;36H([12C)[20;32H[?12l[?25h[?25l( */[?12l[?25h[?25l[46m()[m */[?12l[?25h[?25l[?12l[?25h[?25l[46m([m„Å©„Åì„Åß[46m)[m */[?12l[?25h[?25lËß£Êîæ[46m)[m */[?12l[?25h[?25lÔºü[46m)[m */[?12l[?25h[?25l[20;29H([12C)[18;42H[?12l[?25h[16;42H[14;42H[12;42H[10;42H[8;38H[6;42H[4;41H[2;41H[?25l[1;28r[1;1H[2L[1;29r[1;1H[93m684 [m  if ((i = ArgPos((char *)"-alpha", argc, argv)) > 0) alpha = atof(argv[i + 1]); /*  [2;1H[93m    [margv[i + 1]„ÇídoubleÂûã„Å´Â§âÊèõ„Åó„Å¶alpha„Å´‰ª£ÂÖ• */[28;1H[94m@                                                                                       [2;41H[?12l[?25h[?25l[1;28r[m[1;1H[L[1;29r[1;1H[93m683 [m  if (cbow) alpha = 0.05;[?12l[?25h[?25l[1;28r[1;1H[2L[1;29r[1;1H[93m682 [m  if ((i = ArgPos((char *)"-cbow", argc, argv)) > 0) cbow = atoi(argv[i + 1]); /* arr[2;1H[93m    [mgv[i + 1]„ÇíintÂûã„Å´Â§âÊèõ„Åó„Å¶cbow„Å´‰ª£ÂÖ• */[28;1H[94m@                                                                                       [2;42H[?12l[?25h[?25l[1;28r[m[1;1H[2L[1;29r[1;1H[93m681 [m  if ((i = ArgPos((char *)"-binary", argc, argv)) > 0) binary = atoi(argv[i + 1]); //[2;1H[93m    [m* argv[i + 1]„ÇíintÂûã„Å´Â§âÊèõ„Åó„Å¶binary„Å´‰ª£ÂÖ• */[28;1H[94m@                                                                                       [2;41H[?12l[?25h[?25l[1;28r[m[1;1H[2L[1;29r[1;1H[93m680 [m  if ((i = ArgPos((char *)"-debug", argc, argv)) > 0) debug_mode = atoi(argv[i + 1]))[2;1H[93m    [m; /* argv[i + 1]„ÇíintÂûã„Å´Â§âÊèõ„Åó„Å¶debug_mode„Å´‰
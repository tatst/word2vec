[93m 72 [mvoid ReadWord(char *word, FILE *fin) { /* charå‹ãƒã‚¤ãƒ³ã‚¿wordã¨ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³[21;1H[93m    [mã‚¿finãŒå¼•æ•°ã®voidå‹é–¢æ•°ReadWord() */
[93m 73 [m  int a = 0, ch; /* intå‹aã¨ch */
[93m 74 [m  while (!feof(fin)) { /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finãŒãƒ•ã‚¡ã‚¤ãƒ«ã®çµ‚ç«¯ã«é”ã—ãŸæ™‚ã«ãƒ«[94m>>[m[24;1H[93m    [mãƒ¼ãƒ—çµ‚äº† */[13;34H[?12l[?25h[?25l[25;1H?vocab_size[13;18H[?12l[?25h[?25l[25;1H[3;23H[?12l[?25h[?25l[25;1H[27m[m[H[2J[1;1H[93m 36 [m};
[93m 37 

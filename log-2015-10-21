[93m 40 [mstruct vocab_word *vocab; /* æ§‹é€ ä½“å‹struct vocab_wordã§ãƒã‚¤ãƒ³ã‚¿vocabã‚’å®£è¨€  [8;1H[93m    [m*/
[93m 41 [mint binary = 0, cbow = 1, debug_mode = 2, window = 5, min_count = 5, num_thrr[10;1H[93m    [meads = 12, min_reduce = 1;

[93m252 [m    vocab[a].point[0] = vocab_size - 2;
[93m253 [m    for (b = 0; b < i; b++) {
[93m254 [m      vocab[a].code[i - b - 1] = code[b];
[93m255 [m      vocab[a].point[i - b] = point[b] - vocab_size;
[93m256 [m    }
[93m257 [m  }
[93m258 [m  free(count);

 71 [m// Reads a single word from a file, assuming space + tab + EOL to be word boo[12;1H[93m    [mundaries /* ãƒ•ã‚¡ã‚¤ãƒ«ã‹ã‚‰1å˜èªèª­è¾¼ã¿ï¼Œã‚¹ãƒšãƒ¼ã‚¹ãƒ»ã‚¿ãƒ–ãƒ»è¡Œæœ«ã‚’å˜èªã®åˆ‡ã‚Œç›®ã¨è¦‹[94m>>[m[13;1H[93m    [mãªã™ */
[93m 72 [mvoid ReadWord(char *word, FILE *fin) { /* charå‹ãƒã‚¤ãƒ³ã‚¿wordã¨ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³␀[15;1H[93m    [mã‚¿finãŒå¼•æ•°ã®voidå‹é–¢æ•°ReadWord() */
[93m 73 [m  int a = 0, ch; /* intå‹aã¨ch */
[93m 74 [m  while (!feof(fin)) { /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finãŒãƒ•ã‚¡ã‚¤ãƒ«ã®çµ‚ç«¯ã«é”ã—ãŸæ™‚ã«ãƒ«[94m>>[m[18;1H[93m    [mãƒ¼ãƒ—çµ‚äº† */
[93m 75 [m    ch = fgetc(fin); /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finã‹ã‚‰1æ–‡å­—èª­è¾¼ã‚“ã§intå‹ã§è¿”ã™ */
[93m 76 [m    if (ch == 13) continue;
[93m 77 [m    if ((ch == ' ') || (ch == '\t') || (ch == '\n')) {/* ç©ºç™½ãƒ»ã‚¿ãƒ–ãƒ»æ”¹è¡ŒãŒ[94m>>[m[22;1H[93m    [mã‚ã‚‹å ´åˆ */
[93m 78 [m      if (a > 0) { /* æœ€åˆã¯æ¡ä»¶ã‚’æº€ãŸã•ãªã„ */
[93m 79 [m[7C if (ch == '\n') ungetc(ch, fin); /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finã«1æ–‡å­—è¿”å´ã—[24;1H[94m@                                                                               [m[25;1H[38;5;224msearch hit BOTTOM, continuing at TOP[12;17H[?12l[?25h[14;5H[?25l[m[25;1H/ãƒ•ã‚¡ã‚¤ãƒ«[25;10H[K[25;1H[14;67H[?12l[?25h[?25l[25;1H[17;31H[?12l[?25h[?25l[25;1H[K[25;1H:[?12l[?25h20[?25l[25;3H[K[25;3H[?12l[?25h10[?25l[27m[m[H[2J[1;1H[93m203 [m  long long *parent_node = (long long *)calloc(vocab_size * 2 + 1, sizeof(loo[2;1H[93m    [mng long)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿parent_nodeã«(vocab_size * 2 * 1) * (long ll[3;1H[93m    [mong)åˆ†ã®ãƒ¡ãƒ¢ãƒªã‚’ç¢ºä¿ */
[93m204 [m  for (a = 0; a < vocab_size; a++) count[a] = vocab[a].cn; /* ãƒã‚¤ãƒ³ã‚¿count[94m>>[m[5;1H[93m    [mã®0 <= a < vocab_sizeç•ªç›®ã«ã«vocab[a].cnã‚’ä»£å…¥ */
[93m205 [m  for (a = vocab_size; a < vocab_size * 2; a++) count[a] = 1e15; /* ãƒã‚¤ãƒ³ã‚¿␀[7;1H[93m    [mcountã®vocab_size <= a < vocab_size * 2ç•ªç›®ã«ã«vocab[a].cnã‚’ä»£å…¥ */
[93m206 [m  pos1 = vocab_size - 1;
[93m207 [m  pos2 = vocab_size;
[93m208 [m  // Following algorithm constructs the Huffman tree by adding one node at aa[11;1H[93m    [m time /* ãƒãƒ¼ãƒ‰ã‚’aå›è¿½åŠ ã—ã¦Huffmanæœ¨ã‚’æ§‹æˆã™ã‚‹ã‚¢ãƒ«ã‚´ãƒªã‚ºãƒ  */
[93m209 [m  for (a = 0; a < vocab_size - 1; a++) {
[93m210 [m    // First, find two smallest nodes 'min1, min2' /* ã¾ãš2ã¤ã®æœ€å°ãƒãƒ¼ãƒ‰'mii[14;1H[93m    [mn1, min2'ã‚’æ¢ã™ */
[93m211 [m    if (pos1 >= 0) { /* pos1(æœ€åˆã¯vocab_size-1)ãŒéè² ã®æ™‚ */
[93m212 [m      if (count[pos1] < count[pos2]) { /* count[pos1] < count[pos2](æœ€åˆã¯poo[17;1H[93m    [ms1 = vocab_size - 1, pos2 = vocab_size)ã®æ™‚ */
[93m213 [m[7C min1i = pos1; /* long longå‹min1iã«pos1ã‚’ä»£å…¥ */
[93m214 [m[7C pos1--;
[93m215 [m      } else { /* count[pos1] >= count[pos2]ã®æ™‚ */
[93m216 [m[7C min1i = pos2;
[93m217 [m[7C pos2++;
[93m218 [m      }
[93m219 [m    } else { /* pos1ãŒè² ã®æ™‚ */[13;9H[?12l[?25h[?25l[25;1H/ãƒ•ã‚¡ã‚¤ãƒ«[38;5;224msearch hit BOTTOM, continuing at TOP[27m[m[H[2J[1;1H[93m 61 [m  for (a = 0; a < table_size; a++) {
[93m 62 [m    table[a] = i;
[93m 63 [m    if (a / (double)table_size > d1) {
[93m 64 [m      i++;
[93m 65 [m      d1 += pow(vocab[i].cn, power) / train_words_pow;
[93m 66 [m    }
[93m 67 [m    if (i >= vocab_size) i = vocab_size - 1;
[93m 68 [m  }
[93m 69 [m}

[1;29r[28;1H[93m158 [m      free(vocab[a].word); /* 125è¡Œç›®ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[?12l[?25h[?25l[1;28r[28;1H
[1;29r[28;1H[93m159 [m    } else {[?12l[?25h[?25l[1;28r[1;1H[2M[1;29r[27;1H[93m160 [m      // Hash will be re-computed, as after the sorting it is not actual /* ãƒãƒƒã‚·ãƒ¥[94m>>[m[28;1H[93m    [mã‚’ */[?12l[?25h[?25l[1;28r[28;1H
[1;29r[28;1H[93m161 [m      hash=GetWordHash(vocab[a].word);[?12l[?25h[27;9H[25;16H[24;81H[?25l[46m[[ma[46m][?12l[?25h[?25l[m[a][?12l[?25h[?25l[46m[[ma[46m][?12l[?25h[?25l[m[a][?12l[?25h[?25l 25è¡Œç›®ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;81H[K[24;35H[?12l[?25h[?25l 5è¡Œç›®ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;80H[K[24;35H[?12l[?25h[?25l è¡Œç›®ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;79H[K[24;35H[?12l[?25h[?25l ç›®ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;77H[K[24;35H[?12l[?25h[?25l ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;75H[K[24;35H[?12l[?25h[?25l ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;73H[K[24;35H[?12l[?25h[?25l ä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;71H[K[24;35H[?12l[?25h[?25l ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;69H[K[24;35H[?12l[?25h[?25lãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;67H[K[24;35H[?12l[?25h[?25l vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[24;65H[K[24;35H[?12l[?25h[?25l[29;1H/[?12l[?25hfree([?25l[27m[m[H[2J[1;1H[93m170 [m    vocab[a].code = (char *)calloc(MAX_CODE_LENGTH, sizeof(char));
[93m171 [m    vocab[a].point = (int *)calloc(MAX_CODE_LENGTH, sizeof(int));
[93m172 [m  }
[93m173 [m}
[93m174 
175 [m// Reduces the vocabulary by removing infrequent tokens
[93m176 [mvoid ReduceVocab() {
[93m177 [m  int a, b = 0;
[93m178 [m  unsigned int hash;
[93m179 [m  for (a = 0; a < vocab_size; a++) if (vocab[a].cn > min_reduce) {
[93m180 [m    vocab[b].cn = vocab[a].cn;
[93m181 [m    vocab[b].word = vocab[a].word;
[93m182 [m    b++;
[93m183 [m  } else free(vocab[a].word);
[93m184 [m  vocab_size = b;
[93m185 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;
[93m186 [m  for (a = 0; a < vocab_size; a++) {
[93m187 [m    // Hash will be re-computed, as it is not actual
[93m188 [m    hash = GetWordHash(vocab[a].word);
[93m189 [m    while (vocab_hash[hash] != -1) hash = (hash + 1) % vocab_hash_size;
[93m190 [m    vocab_hash[hash] = a;
[93m191 [m  }
[93m192 [m  fflush(stdout);
[93m193 [m  min_reduce++;
[93m194 [m}
[93m195 
196 [m// Create binary Huffman tree using the word counts /* èªæ•°ã‚’ç”¨ã„ã¦2å€¤Huffmanæœ¨ã‚’ä½œæˆ[28;1H[93m    [m */[14;14H[?12l[?25h[?25l[29;1H:[?12l[?25hw[?25l"word2vec.c" 703L, 34928C written[14;14H[?12l[?25h[?25l[29;1H[K[29;1H:[?12l[?25hq[?25l[29;1H[K[29;1H[?1l>[?12l[?25h[?1049lbash-3.2$ exit
exit


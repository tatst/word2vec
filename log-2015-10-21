[93m156 [m    if ((vocab[a].cn < min_count) && (a != 0)) { /* vocab[a].cnãŒmin_countæœªæº€ã‹ã¤aãŒ[28;1H[93m    [m0ã§ãªã„æ™‚ */[14;35H[?12l[?25h[?25l[29;1H?SortVocab[14;10H[?12l[?25h[?25l[29;1H[38;5;224msearch hit TOP, continuing at BOTTOM[27m[m[H[2J[1;1H[93m311 [m  if (fin == NULL) {

[93m159 [m    } else {
[93m160 [m      // Hash will be re-computed, as after the sorting it is not actual /*  [4;1H[93m    [mãƒãƒƒã‚·ãƒ¥ã‚’ */[25;1H[K[1;35H[?12l[?25h[?25l[25;1H?vocab[1;16H[?12l[?25h[?25l[25;1H[1;24r[1;1H[L[1;25r[1;1H[93m157 [m      vocab_size--; /* vocab_sizeã‹ã‚‰1å¼•ã */[25;1H[K[1;28H[?12l[?25h[?25l[25;1H?vocab[1;11H[?12l[?25h[?25l[25;1H[1;24r[1;1H[2L[1;25r[1;1H[93m156 [m    if ((vocab[a].cn < min_count) && (a != 0)) { /* vocab[a].cnãŒmin_countæœª[2;1H[93m    [mæº€ã‹ã¤aãŒ0ã§ãªã„æ™‚ */[25;1H[K[1;57H[?12l[?25h[?25l[25;1H?vocab[1;14H[?12l[?25h[?25l[25;1H/[1;57H[?12l[?25h[?25l[25;1H[3;11H[?12l[?25h[?25l[25;1H[3;28H[?12l[?25h[?25l[25;1H[4;16H[?12l[?25h[?25l[25;1H[4;35H[?12l[?25h[?25l[25;1H[8;28H[?12l[?25h[?25l[25;1H[9;18H[?12l[?25h[?25l[25;1H[9;62H[?12l[?25h[?25l[25;1H?[9;18H[?12l[?25h[?25l[25;1H[8;28H[?12l[?25h[?25l[25;1H[4;35H[?12l[?25h[?25l[25;1H[4;16H[?12l[?25h[?25l[25;1H[3;28H[?12l[?25h[?25l[25;1H[3;11H[?12l[?25h[?25l[25;1H[1;57H[?12l[?25h[?25l[25;1H[1;14H[?12l[?25h[?25l[25;1H[1;24r[1;1H[2L[1;25r[1;1H[93m155 [m    // Words occuring less than min_count times will be discarded from the vv[2;1H[93m    [mocab /* min_countä»¥ä¸‹ã®é »åº¦ã®å˜èªã‚’vocabã‹ã‚‰é™¤å¤–ã™ã‚‹ */[25;1H[K[2;40H[?12l[?25h[?25l[25;1H?vocab[1;80H[?12l[?25h[?25l[25;1H[1;24r[1;1H[3L[1;25r[1;1H[93m152 [m  size = vocab_size;
[93m153 [m  train_words = 0;
[93m154 [m  for (a = 0; a < size; a++) {[25;1H[K[1;14H[?12l[?25h[?25l[25;1H?vocab[1;24r[1;1H[L[1;25r[1;1H[93m151 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;[25;1H[K[1;45H[?12l[?25h[?25l[25;1H?vocab[1;23H[?12l[?25h[?25l[25;1H[1;24r[1;1H[2L[1;25r[1;1H[93m150 [m  qsort(&vocab[1], vocab_size - 1, sizeof(struct vocab_word), VocabCompare);;[2;1H[93m    [m/* vocab[1]ã®ã‚¢ãƒ‰ãƒ¬ã‚¹ï¼Œvocab_size -1ï¼Œ */[25;1H[K[2;28H[?12l[?25h[?25l[25;1H?vocab[2;8H[?12l[?25h[?25l[25;1H[1;54H[?12l[?25h[?25l[25;1H[1;24H[?12l[?25h[?25l[25;1H[1;14H[?12l[?25h[?25l[25;1H[1;24r[1;1H[2L[1;25r[1;1H[93m149 [m  // Sort the vocabulary and keep </s> at the first position /* èªå½™ã‚’ä¸¦æ›¿ãˆ[2;1H[93m    [mã¦æ–‡å­—åˆ—</s>ã‚’å…ˆé ­ã«ä¿ã¤ */[24;1H[94m@                                                                               [m[25;1H[K[1;19H[?12l[?25h[?25l[25;1H?vocab[1;24r[1;1H[5L[1;25r[1;1H[93m145 [m// Sorts the vocabulary by frequency using word counts /* èªå½™ã‚’å˜èªæ•°ã‚’ç”¨ã„[2;1H[93m    [mã¦é »åº¦é †ã«ä¸¦æ›¿ãˆ */
[93m146 [mvoid SortVocab() { /* voidé–¢æ•°SortVocab() */

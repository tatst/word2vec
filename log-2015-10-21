[93m 72 [mvoid ReadWord(char *word, FILE *fin) { /* charå‹ãƒã‚¤ãƒ³ã‚¿wordã¨ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finãŒå¼•␀[12;1H[93m    [mæ•°ã®voidå‹é–¢æ•°ReadWord() */
[93m 73 [m  int a = 0, ch; /* intå‹aã¨ch */
[93m 74 [m  while (!feof(fin)) { /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finãŒãƒ•ã‚¡ã‚¤ãƒ«ã®çµ‚ç«¯ã«é”ã—ãŸæ™‚ã«ãƒ«ãƒ¼ãƒ—çµ‚äº† **[15;1H[93m    [m/
[93m 75 [m    ch = fgetc(fin); /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finã‹ã‚‰1æ–‡å­—èª­è¾¼ã‚“ã§intå‹ã§è¿”ã™ */
[93m 76 [m    if (ch == 13) continue;
[93m 77 [m    if ((ch == ' ') || (ch == '\t') || (ch == '\n')) {/* ç©ºç™½ãƒ»ã‚¿ãƒ–ãƒ»æ”¹è¡ŒãŒã‚ã‚‹å ´åˆ **[19;1H[93m    [m/
[93m 78 [m      if (a > 0) { /* æœ€åˆã¯æ¡ä»¶ã‚’æº€ãŸã•ãªã„ */
[93m 79 [m[7C if (ch == '\n') ungetc(ch, fin); /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finã«1æ–‡å­—è¿”å´ã—chã‚’è¿”ã™  [22;1H[93m    [m*/
[93m 80 [m[7C break;
[93m 81 [m      }
[93m 82 [m      if (ch == '\n') { /* æ”¹è¡ŒãŒã‚ã‚‹å ´åˆ */

[93m532 [m    }
[93m533 [m    sentence_position++;
[93m534 [m    if (sentence_position >= sentence_length) {
[93m535 [m      sentence_length = 0;
[93m536 [m      continue;
[93m537 [m    }
[93m538 [m  }
[93m539 [m  fclose(fi);
[93m540 [m  free(neu1);
[93m541 [m  free(neu1e);
[93m542 [m  pthread_exit(NULL);
[93m543 [m}

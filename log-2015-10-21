[93m 99 [m  return hash; /* hashã‚’è¿”ã™ */
[93m100 [m}
[93m101 
102 [m// Returns position of a word in the vocabulary; if the word is not found, rr[7;1H[93m    [meturns -1 /* èªå½™ä¸­ã®å˜èªã®ä½ç½®ã‚’è¿”ã™(å˜èªãŒèªå½™ä¸­ã«ç„¡ã„å ´åˆã¯-1ã‚’è¿”ã™) */
[93m103 [mint SearchVocab(char *word) { /* charå‹ãƒã‚¤ãƒ³ã‚¿wordã‚’å¼•æ•°ã«æŒã¤intå‹é–¢æ•°Searr[9;1H[93m    [mchVocab() */
[93m104 [m  unsigned int hash = GetWordHash(word); /* ç¬¦å·ç„¡intå‹hashã«wordã®hashå€¤ã‚’[94m>>[m[11;1H[93m    [mä»£å…¥ */
[93m105 [m  while (1) { /* ç„¡é™ãƒ«ãƒ¼ãƒ— */
[93m106 [m    if (vocab_hash[hash] == -1) return -1; /* vocab_hash[hash]ãŒ-1ã®æ™‚-1ã‚’è¿”[14;1H[93m    [mã™ */
[93m107 [m    if (!strcmp(word, vocab[vocab_hash[hash]].word)) return vocab_hash[hash]][16;1H[93m    [m; /* æ–‡å­—åˆ—wordã¨vocab[vocab_hash[hash]].wordãŒç­‰ã—ã„æ™‚vocab_hash[hash]ã‚’è¿”[94m>>[m[17;1H[93m    [mã™ */
[93m108 [m    hash = (hash + 1) % vocab_hash_size; /* hashã«(hash + 1) % vocab_hash_sii[19;1H[93m    [mzeã‚’è¿”ã™ */
[93m109 [m  }
[93m110 [m  return -1;
[93m111 [m}
[93m112 

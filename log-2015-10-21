[93m206 [m  pos1 = vocab_size - 1;
[93m207 [m  pos2 = vocab_size;
[93m208 [m  // Following algorithm constructs the Huffman tree by adding one node at aa[11;1H[93m    [m time /* ãƒãƒ¼ãƒ‰ã‚’aå›è¿½åŠ ã—ã¦Huffmanæœ¨ã‚’æ§‹æˆã™ã‚‹ã‚¢ãƒ«ã‚´ãƒªã‚ºãƒ  */
[93m209 [m  for (a = 0; a < vocab_size - 1; a++) {
[93m210 [m    // First, find two smallest nodes 'min1, min2' /* ã¾ãš2ã¤ã®æœ€å°ãƒãƒ¼ãƒ‰'mii[14;1H[93m    [mn1, min2'ã‚’æ¢ã™ */
[93m211 [m    if (pos1 >= 0) { /* pos1(æœ€åˆã¯vocab_size-1)ãŒéè² ã®æ™‚ */
[93m212 [m      if (count[pos1] < count[pos2]) { /* count[pos1] < count[pos2](æœ€åˆã¯poo[17;1H[93m    [ms1 = vocab_size - 1, pos2 = vocab_size)ã®æ™‚ */
[93m213 [m[7C min1i = pos1; /* long longå‹min1iã«pos1ã‚’ä»£å…¥ */
[93m214 [m[7C pos1--;
[93m215 [m      } else { /* count[pos1] >= count[pos2]ã®æ™‚ */
[93m216 [m[7C min1i = pos2;
[93m217 [m[7C pos2++;
[93m218 [m      }
[93m219 [m    } else { /* pos1ãŒè² ã®æ™‚ */[13;9H[?12l[?25h[15;9H[16;9H[18;9H[19;9H[20;9H[21;9H[22;9H[23;9H[?25l[15;24H[46m{[24;9H}[?12l[?25h[?25l[1;24r[m[1;1H[3M[1;25r[12;24H{[21;9H}
[93m220 [m      min1i = pos2;
[93m221 [m      pos2++;
[93m222 [m    }[22;9H[?12l[?25h[23;9H[?25l[21;16H[46m{[24;9H}[?12l[?25h[?25l[1;24r[m[1;1H[2M[1;25r[19;16H{[22;9H}
[93m223 [m    if (pos1 >= 0) {
[93m224 [m      if (count[pos1] < count[pos2]) { /* count[pos1] < count[pos2](æœ€åˆã¯po[24;1H[94m@                                                                               [23;9H[?12l[?25h[?25l[1;24r[m[1;1H[2M[1;25r[22;1H[93m224 [m      if (count[pos1] < count[pos2]) { /* count[pos1] < count[pos2](æœ€åˆã¯poo[23;1H[93m    [mos1 = vocab_size - 1, pos2 = vocab_size)ã®æ™‚ */

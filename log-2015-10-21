[93m284 [m    if (i == -1) {
[93m285 [m      a = AddWordToVocab(word);
[93m286 [m      vocab[a].cn = 1;[12;7H[?12l[?25hAddWordToVoca[?25lb[46m([14C)[12;21H[?12l[?25h[?25l[m([46m([mchar *[46m)[m"</s>")[12;22H[?12l[?25h[?25l(char *)[12;23H[?12l[?25hchar [?25l[46m([mchar *[46m)[?12l[?25h[?25l[m[12;22H(char *)[?12l[?25h"</[?25l[25;1H?AddWordToVocab[12;7H[?12l[?25h[?25l[25;1H[27m[m[H[2J[1;1H[93m114 [mint ReadWordIndex(FILE *fin) { /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿finã‚’å¼•æ•°ã«æŒã¤intå‹é–¢æ•°Ree[2;1H[93m    [madWordIndex() */

[93m382 [m[8C word_count_actual / ((real)(now - start + 1) / (real)CLOCKS_PER_SEC * 1000)))[23;1H[93m    [m;
[93m383 [m[7C fflush(stdout);
[93m384 [m      }
[93m385 [m      alpha = starting_alpha * (1 - word_count_actual / (real)(iter * train_words + 11[27;1H[93m    [m));

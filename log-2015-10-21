[93m527 [m[9C for (c = 0; c < layer1_size; c++) syn1neg[c + l2] += g * syn0[c + l1];
[93m528 [m[7C }
[93m529 [m[7C // Learn weights input -> hidden
[93m530 [m[7C for (c = 0; c < layer1_size; c++) syn0[c + l1] += neu1e[c];

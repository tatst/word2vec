[93m195 
196 [m// Create binary Huffman tree using the word counts /* èªæ•°ã‚’ç”¨ã„ã¦2å€¤Huffmaa[7;1H[93m    [mnæœ¨ã‚’ä½œæˆ */
[93m197 [m// Frequent words will have short uniqe binary codes /* é«˜é »åº¦ã®å˜èªã«çŸ­ã„ä¸€[9;1H[93m    [mæ„çš„ãª2é€²æ•°ã‚’å‰²å½“ã‚‹ */
[93m198 [mvoid CreateBinaryTree() { /* voidé–¢æ•°CreateBinaryTree() */
[93m199 [m  long long a, b, i, min1i, min2i, pos1, pos2, point[MAX_CODE_LENGTH];
[93m200 [m  char code[MAX_CODE_LENGTH]; /* charå‹code[æœ€å¤§ã‚³ãƒ¼ãƒ‰é•·MAX_CODE_LENGTH] */
[93m201 [m  long long *count = (long long *)calloc(vocab_size * 2 + 1, sizeof(long lonn[14;1H[93m    [mg)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿countã«(vocab_size * 2 * 1) * (long long)åˆ†ã®ãƒ¡ãƒ¢␀[15;1H[93m    [mãƒªã‚’ç¢ºä¿ */
[93m202 [m  long long *binary = (long long *)calloc(vocab_size * 2 + 1, sizeof(long loo[17;1H[93m    [mng)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿binaryã«(vocab_size * 2 * 1) * (long long)åˆ†ã®ãƒ¡␀[18;1H[93m    [mãƒ¢ãƒªã‚’ç¢ºä¿ */
[93m203 [m  long long *parent_node = (long long *)calloc(vocab_size * 2 + 1, sizeof(loo[20;1H[93m    [mng long)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿parent_nodeã«(vocab_size * 2 * 1) * (long ll[21;1H[93m    [mong)åˆ†ã®ãƒ¡ãƒ¢ãƒªã‚’ç¢ºä¿ */
[93m204 [m  for (a = 0; a < vocab_size; a++) count[a] = vocab[a].cn;
[93m205 [m  for (a = vocab_size; a < vocab_size * 2; a++) count[a] = 1e15;

[27m[m[H[2J[1;1H[93m117 [m  if (feof(fin)) return -1; /* ãƒ•ã‚¡ã‚¤ãƒ«ãƒã‚¤ãƒ³ã‚¿ãŒçµ‚ç«¯ã«é”ã—ãŸæ™‚-1ã‚’è¿”ã™ */
[93m118 [m  return SearchVocab(word); /* å˜èªwordã®èªå½™ä¸­ã§ã®ä½ç½®ã‚’è¿”ã™ */
[93m119 [m}
[93m120 
121 [m// Adds a word to the vocabulary /* å˜èªã‚’èªå½™ã«åŠ ãˆã‚‹ */
[93m122 [mint AddWordToVocab(char *word) { /* charå‹ãƒã‚¤ãƒ³ã‚¿wordã‚’å¼•æ•°ã«æŒã¤intå‹é–¢æ•°AA[7;1H[93m    [mddWordToVocab() */
[93m123 [m  unsigned int hash, length = strlen(word) + 1; /* ç¬¦å·ç„¡intå‹bash, length(ww[9;1H[93m    [mordã®æ–‡å­—é•·+1ã‚’ä»£å…¥) */
[93m124 [m  if (length > MAX_STRING) length = MAX_STRING; /* lengthãŒæœ€å¤§æ–‡å­—æ•°ã‚ˆã‚Šå¤§[94m>>[m[11;1H[93m    [mãã„å ´åˆã¯lenghã«æœ€å¤§æ–‡å­—æ•°ã‚’ä»£å…¥ */
[93m125 [m  vocab[vocab_size].word = (char *)calloc(length, sizeof(char)); /* lengthå€‹[13;1H[93m    [mã®charã‚µã‚¤ã‚ºã®ãƒ¡ãƒ¢ãƒªã‚’ç¢ºä¿ã—ï¼Œcharå‹ã«ã—ã¦vocab[vocab_size].wordã«ä»£å…¥ */
[93m126 [m  strcpy(vocab[vocab_size].word, word); /* vocab[vocab_size].wordã«wordã‚’ã‚³[94m>>[m[15;1H[93m    [mãƒ”ãƒ¼ */
[93m127 [m  vocab[vocab_size].cn = 0; /* vocab[vocab_size].cnã«0ã‚’ä»£å…¥ */
[93m128 [m  vocab_size++; /* vocab_sizeã«1ã‚’è¶³ã™ */
[93m129 [m  // Reallocate memory if needed /* å¿…è¦æ™‚ã«ãƒ¡ãƒ¢ãƒªã®å‰²å½“ã‚’å¤‰æ›´ */
[93m130 [m  if (vocab_size + 2 >= vocab_max_size) { /* vocab_size + 2ãŒvocab_max_size[94m>>[m[20;1H[93m    [mä»¥ä¸Šã®æ™‚ */
[93m131 [m    vocab_max_size += 1000; /* vocab_max_sizeã«1000ã‚’è¶³ã™ */
[93m132 [m    vocab = (struct vocab_word *)realloc(vocab, vocab_max_size * sizeof(struu[23;1H[93m    [mct vocab_word)); /*  */
[93m133 [m  }[12;13H[?12l[?25h[?25l[25;1H/vocab_size[13;53H[?12l[?25h[?25l[25;1H[14;20H[?12l[?25h[?25l[25;1H[14;54H[?12l[?25h[?25l[25;1H[16;13H[?12l[?25h[?25l[25;1H[16;42H[?12l[?25h[?25l[25;1H[17;7H[?12l[?25h[?25l[25;1H[17;24H[?12l[?25h[?25l[25;1H[19;11H[?12l[?25h[?25l[25;1H[19;50H[?12l[?25h[?25l[25;1H[1;24r[1;1H[3M[1;25r[22;1H[93m134 [m  hash = GetWordHash(word); /* hashã«wordã®hashã‚’ä»£å…¥ */
[93m135 [m  while (vocab_hash[hash] != -1) hash = (hash + 1) % vocab_hash_size;
[93m136 [m  vocab_hash[hash] = vocab_size - 1;[25;1H[K[24;26H[?12l[?25h[?25l
/vocab_size[1;24r[24;1H
[1;25r[24;1H[93m137 [m  return vocab_size - 1;[25;1H[K[24;14H[?12l[?25h[?25l
/vocab_size[27m[m[H[2J[1;1H[93m141 [mint VocabCompare(const void *a, const void *b) {
[93m142 [m    return ((struct vocab_word *)b)->cn - ((struct vocab_word *)a)->cn;
[93m143 [m}
[93m144 
145 [m// Sorts the vocabulary by frequency using word counts /* èªå½™ã‚’å˜èªæ•°ã‚’ç”¨ã„[6;1H[93m    [mã¦é »åº¦é †ã«ä¸¦æ›¿ãˆ */
[93m146 [mvoid SortVocab() { /* voidé–¢æ•°SortVocab() */
[93m147 [m  int a, size;
[93m148 [m  unsigned int hash;
[93m149 [m  // Sort the vocabulary and keep </s> at the first position /* èªå½™ã‚’ä¸¦æ›¿ãˆ[11;1H[93m    [mã¦æ–‡å­—åˆ—</s>ã‚’å…ˆé ­ã«ä¿ã¤ */
[93m150 [m  qsort(&vocab[1], vocab_size - 1, sizeof(struct vocab_word), VocabCompare);;[13;1H[93m    [m/* vocab[1]ã®ã‚¢ãƒ‰ãƒ¬ã‚¹ï¼Œvocab_size -1ï¼Œ */
[93m151 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;
[93m152 [m  size = vocab_size;
[93m153 [m  train_words = 0;
[93m154 [m  for (a = 0; a < size; a++) {
[93m155 [m    // Words occuring less than min_count times will be discarded from the vv[19;1H[93m    [mocab /* min_countä»¥ä¸‹ã®é »åº¦ã®å˜èªã‚’vocabã‹ã‚‰é™¤å¤–ã™ã‚‹ */
[93m156 [m    if ((vocab[a].cn < min_count) && (a != 0)) { /* vocab[a].cnãŒmin_countæœª[21;1H[93m    [mæº€ã‹ã¤aãŒ0ã§ãªã„æ™‚ */
[93m157 [m      vocab_size--; /* vocab_sizeã‹ã‚‰1å¼•ã */
[93m158 [m      free(vocab[a].word); /* vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */
[93m159 [m    } else {[12;24H[?12l[?25h[?25l[25;1H/vocab_size[13;28H[?12l[?25h[?25l[25;1H[15;14H[?12l[?25h[?25l[25;1H[22;11H[?12l[?25h[?25l


[22;28H[?12l[?25h[?25l


[1;24r[1;1H[11M[1;25r[14;1H[93m160 [m      // Hash will be re-computed, as after the sorting it is not actual /*  [15;1H[93m    [mãƒãƒƒã‚·ãƒ¥ã‚’ */
[93m161 [m      hash=GetWordHash(vocab[a].word);
[93m162 [m      while (vocab_hash[hash] != -1) hash = (hash + 1) % vocab_hash_size;
[93m163 [m      vocab_hash[hash] = a;
[93m164 [m      train_words += vocab[a].cn;
[93m165 [m    }
[93m166 [m  }
[93m167 [m  vocab = (struct vocab_word *)realloc(vocab, (vocab_size + 1) * sizeof(struu[23;1H[93m    [mct vocab_word));
[93m168 [m  // Allocate memory for the binary tree construction[25;1H[K[22;52H[?12l[?25h[?25l


/vocab_size[1;24r[1;1H[2M[1;25r[23;1H[93m169 [m  for (a = 0; a < vocab_size; a++) {
[93m170 [m    vocab[a].code = (char *)calloc(MAX_CODE_LENGTH, sizeof(char));[25;1H[K[23;23H[?12l[?25h[?25l

/vocab_size[1;24r[1;1H[9M[1;25r[16;1H[93m171 [m    vocab[a].point = (int *)calloc(MAX_CODE_LENGTH, sizeof(int));
[93m172 [m  }
[93m173 [m}
[93m174 
175 [m// Reduces the vocabulary by removing infrequent tokens
[93m176 [mvoid ReduceVocab() {
[93m177 [m  int a, b = 0;
[93m178 [m  unsigned int hash;
[93m179 [m  for (a = 0; a < vocab_size; a++) if (vocab[a].cn > min_reduce) {[25;1H[K[24;23H[?12l[?25h[?25l
/vocab_size[1;24r[1;1H[5M[1;25r[20;1H[93m180 [m    vocab[b].cn = vocab[a].cn;
[93m181 [m    vocab[b].word = vocab[a].word;
[93m182 [m    b++;
[93m183 [m  } else free(vocab[a].word);
[93m184 [m  vocab_size = b;[25;1H[K[24;7H[?12l[?25h[?25l
/vocab_size[1;24r[1;1H[2M[1;25r[23;1H[93m185 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;
[93m186 [m  for (a = 0; a < vocab_size; a++) {[25;1H[K[24;23H[?12l[?25h[?25l
/vocab_size[27m[m[H[2J[1;1H[93m193 [m  min_reduce++;
[93m194 [m}
[93m195 
196 [m// Create binary Huffman tree using the word counts /* èªæ•°ã‚’ç”¨ã„ã¦2å€¤Huffmaa[5;1H[93m    [mnæœ¨ã‚’ä½œæˆ */
[93m197 [m// Frequent words will have short uniqe binary codes /* é«˜é »åº¦ã®å˜èªã«çŸ­ã„ä¸€␀[7;1H[93m    [mæ„çš„ãª2é€²æ•°ã‚’å‰²å½“ã‚‹ */
[93m198 [mvoid CreateBinaryTree() { /* voidé–¢æ•°CreateBinaryTree() */
[93m199 [m  long long a, b, i, min1i, min2i, pos1, pos2, point[MAX_CODE_LENGTH];
[93m200 [m  char code[MAX_CODE_LENGTH]; /* charå‹code[æœ€å¤§ã‚³ãƒ¼ãƒ‰é•·MAX_CODE_LENGTH] */
[93m201 [m  long long *count = (long long *)calloc(vocab_size * 2 + 1, sizeof(long lonn[12;1H[93m    [mg)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿countã«(vocab_size * 2 * 1) * (long long)åˆ†ã®ãƒ¡ãƒ¢[13;1H[93m    [mãƒªã‚’ç¢ºä¿ */
[93m202 [m  long long *binary = (long long *)calloc(vocab_size * 2 + 1, sizeof(long loo[15;1H[93m    [mng)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿binaryã«(vocab_size * 2 * 1) * (long long)åˆ†ã®ãƒ¡[16;1H[93m    [mãƒ¢ãƒªã‚’ç¢ºä¿ */
[93m203 [m  long long *parent_node = (long long *)calloc(vocab_size * 2 + 1, sizeof(loo[18;1H[93m    [mng long)); /* long longå‹ãƒã‚¤ãƒ³ã‚¿parent_nodeã«(vocab_size * 2 * 1) * (long ll[19;1H[93m    [mong)åˆ†ã®ãƒ¡ãƒ¢ãƒªã‚’ç¢ºä¿ */
[93m204 [m  for (a = 0; a < vocab_size; a++) count[a] = vocab[a].cn; /* ãƒã‚¤ãƒ³ã‚¿count[94m>>[m[21;1H[93m    [mã®0 <= a < vocab_sizeç•ªç›®ã«ã«vocab[a].cnã‚’ä»£å…¥ */
[93m205 [m  for (a = vocab_size; a < vocab_size * 2; a++) count[a] = 1e15; /* ãƒã‚¤ãƒ³ã‚¿␀[23;1H[93m    [mcountã®vocab_size <= a < vocab_size * 2ç•ªç›®ã«ã«vocab[a].cnã‚’ä»£å…¥ */
[93m206 [m  pos1 = vocab_size - 1;[11;46H[?12l[?25h[?25l[34Cnn[12;1H[93m [m[14;46H[46m([m[33Coo[15;1H[93m [m[6C[46m)[14;46H[?12l[?25h[?25l[m([33Coo[15;1H[93m [m[6C)[17;46H[?12l[?25h[20;46H[22;46H[24;28H[?25l[1;24r[24;1H
[1;25r[24;1H[93m207 [m  pos2 = vocab_size;[?12l[?25h[?25l[1;24r[1;1H[2M[1;25r[23;1H[93m208 [m  // Following algorithm constructs the Huffman tree by adding one node at aa[24;1H[93m    [m time /* ãƒãƒ¼ãƒ‰ã‚’aå›è¿½åŠ ã—ã¦Huffmanæœ¨ã‚’æ§‹æˆã™ã‚‹ã‚¢ãƒ«ã‚´ãƒªã‚ºãƒ  */[23;46H[?12l[?25h[22;24H[21;28H[19;46H[21;28H[22;24H[23;46H[?25l[1;24r[1;1H[2M[1;25r[23;1H[93m209 [m  for (a = 0; a < vocab_size - 1; a++) {
[93m210 [m    // First, find two smallest nodes 'min1, min2' /* ã¾ãš2ã¤ã®æœ€å°ãƒãƒ¼ãƒ‰'mi[24;1H[94m@                                                                               [23;44H[?12l[?25h[?25l[1;24r[m[1;1H[2M[1;25r[22;1H[93m210 [m    // First, find two smallest nodes 'min1, min2' /* ã¾ãš2ã¤ã®æœ€å°ãƒãƒ¼ãƒ‰'mii[23;1H[93m    [mn1, min2'ã‚’æ¢ã™ */
[93m211 [m    if (pos1 >= 0) { /* pos1(æœ€åˆã¯vocab_size-1)ãŒéè² ã®æ™‚ */[22;46H[?12l[?25h[24;46H[?25l[1;24r[24;1H
[1;25r[24;1H[93m212 [m      if (count[pos1] < count[pos2]) {[?12l[?25h[?25l[1;24r[24;1H
[1;25r[24;1H[93m213 [m[7C min1i = pos1;[?12l[?25h[?25l[1;24r[24;1H

[93m 44 [mlong long train_words = 0, word_count_actual = 0, iter = 5, file_size = 0, cc[24;1H[93m    [mlasses = 0;[25;1H[K[23;37H[?12l[?25h[?25l

/count[27m[m[H[2J[1;1H[93m130 [m  if (vocab_size + 2 >= vocab_max_size) { /* vocab_size + 2ãŒvocab_max_size[94m>>[m[2;1H[93m    [mä»¥ä¸Šã®æ™‚ */
[93m131 [m    vocab_max_size += 1000; /* vocab_max_sizeã«1000ã‚’è¶³ã™ */

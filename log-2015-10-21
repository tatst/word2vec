[1;25r[24;1H[93m241 [m  for (a = 0; a < vocab_size; a++) { /* 0 <= a < vocab_sizeã®æ™‚ */[24;9H[?12l[?25h[?25l[1;24r[24;1H
[1;25r[24;1H[93m242 [m    b = a;[?12l[?25h[?25l[1;24r[1;1H[2M[1;25r[23;1H[93m243 [m    i = 0;
[93m244 [m    while (1) { /* ç„¡é™ãƒ«ãƒ¼ãƒ— */[23;9H[?12l[?25h[24;9H[?25l[1;24r[24;1H
[1;25r[24;1H[93m245 [m      code[i] = binary[b]; /* code[i]ã«binary[b](æœ€åˆã¯b=a)ã‚’ä»£å…¥ */[24;9H[?12l[?25h[?25l[1;24r[24;1H

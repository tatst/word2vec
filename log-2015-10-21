[1;25r[24;1H[93m252 [m    vocab[a].point[0] = vocab_size - 2;[24;9H[?12l[?25h[?25l[1;24r[24;1H
[1;25r[24;1H[93m253 [m    for (b = 0; b < i; b++) {[24;9H[?12l[?25h[?25l[1;24r[1;1H[2M[1;25r[23;1H[93m254 [m      vocab[a].code[i - b - 1] = code[b];

[93m264 [m  char word[MAX_STRING];
[93m265 [m  FILE *fin;
[93m266 [m  long long a, i;
[93m267 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;
[93m268 [m  fin = fopen(train_file, "rb");
[93m269 [m  if (fin == NULL) {
[93m270 [m    printf("ERROR: training data file not found!\n");[12;7H[?12l[?25hfre[?25le[46m([mcount[46m)[12;11H[?12l[?25h[?25l[m(count)[?12l[?25hcoun[?25l[46m([mcount[46m)[?12l[?25h[?25l[m(count)[?12l[?25h[?25l[25;1H[1m-- INSERT --[12;19H[?12l[?25h[?25l[m [?12l[?25h[?25l ?[?12l[?25h[?25l[12;20H[K[12;20H[?12l[?25h[?25l /[?12l[?25h[?25l/*[?12l[?25h[?25l [?12l[?25h[?25l [?12l[?25h[?25l *[?12l[?25h[?25l*/[?12l[?25h[?25l c */[?12l[?25h[?25lco */[?12l[?25h[?25lou */[?12l[?25h[?25lut */[?12l[?25h[?25lu */[12;29H[K[12;26H[?12l[?25h[?25lun */[?12l[?25h[?25lnt */[?12l[?25h[?25ltåˆ† */[?12l[?25h[?25lã® */[?12l[?25h[?25lãƒ¡ãƒ¢ãƒª */[?12l[?25h[?25lã‚’ */[?12l[?25h[?25lè§£æ”¾ */[?12l[?25h[?25l[11;8H[?12l[?25h[12;44H[13;20H[?25l [?12l[?25h[?25l /[?12l[?25h[?25l/*[?12l[?25h[?25l [?12l[?25h[?25l [?12l[?25h[?25l *[?12l[?25h[?25l*/[?12l[?25h[?25l i */[?12l[?25h[?25l  */[13;27H[K[13;24H[?12l[?25h[?25l b */[?12l[?25h[?25lbi */[?12l[?25h[?25lin */[?12l[?25h[?25lna */[?12l[?25h[?25lar */[?12l[?25h[?25lry */[?12l[?25h[?25lyåˆ†ã® */[?12l[?25h[?25lãƒ¡ãƒ¢ãƒª */[?12l[?25h[?25lã‚’ */[?12l[?25h[?25lè§£æ”¾ */[?12l[?25h[14;25H[?25l [?12l[?25h[?25l /[?12l[?25h[?25l/+[?12l[?25h[?25l[14;27H[K[14;27H[?12l[?25h[?25l/*[?12l[?25h[?25l [?12l[?25h[?25l [?12l[?25h[?25l *[?12l[?25h[?25l*/[?12l[?25h[?25l p */[?12l[?25h[?25lpa */[?12l[?25h[?25lar */[?12l[?25h[?25lre */[?12l[?25h[?25len */[?12l[?25h[?25lnt */[?12l[?25h[?25lt_ */[?12l[?25h[?25l_n */[?12l[?25h[?25lno */[?12l[?25h[?25lod */[?12l[?25h[?25lde */[?12l[?25h[?25leåˆ† */[?12l[?25h[?25lã® */[?12l[?25h[?25lãƒ¡ãƒ¢ãƒª */[?12l[?25h[?25lã‚’ */
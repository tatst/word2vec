[1;29r[28;1H[93m185 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;[28;52H[?12l[?25h[27;21H[26;33H[25;12H[24;38H[23;34H[22;52H[21;24H[20;19H[19;24H[18;52H[17;5H[16;5H[?25l[12;40H[46m{[15;7H}[?12l[?25h[?25l[m[12;40H{[15;7H}[14;52H[?12l[?25h[13;52H[?25l[12;40H[46m{[15;7H}[12;40H[?12l[?25h[?25l[m{[15;7H}[11;52H[?12l[?25h[9;52H[8;7H[7;9H[6;37H[5;31H[4;52H[3;42H[1;52H[?25l[1;28r[1;1H[L[1;29r[1;1H[93m159 [m    } else [46m{[8;9H}[1;16H[?12l[?25h[?25l[1;28r[m[1;1H[L[1;29r[1;1H[93m158 [m      free(vocab[a].word); /* 125è¡Œç›®ã§ç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[2;16H{[9;9H}[1;52H[?12l[?25h[?25l[1;28r[1;1H[L[1;29r[1;1H[93m157 [m      vocab_size--; /* vocab_sizeã‹ã‚‰1å¼•ã */[?12l[?25h[?25l[1;28r[1;1H[2L[1;29r[1;1H[93m156 [m    if ((vocab[a].cn < min_count) && (a != 0)) [46m{[m /* vocab[a].cnãŒmin_countæœªæº€ã‹ã¤aãŒ[2;1H[93m    [m0ã§ãªã„æ™‚ */[5;9H[46m}[1;52H[?12l[?25h[?25l[1;28r[m[1;1H[2L[1;29r[1;1H[93m155 [m    // Words occuring less than min_count times will be discarded from the vocab /* mm[2;1H[93m    [min_countä»¥ä¸‹ã®é »åº¦ã®å˜èªã‚’vocabã‹ã‚‰é™¤å¤–ã™ã‚‹ */[3;52H{[7;9H}[1;52H[?12l[?25h[?25l[1;28r[1;1H[L[1;29r[1;1H[93m154 [m  for (a = 0; a < size; a++) [46m{[16;7H}[1;34H[?12l[?25h[?25l[1;28r[m[1;1H[L[1;29r[1;1H[93m153 [m  train_words = 0;[2;34H{[17;7H}[1;22H[?12l[?25h[?25l[1;28r[1;1H[L[1;29r[1;1H[93m152 [m  size = vocab_size;[?12l[?25h[?25l[1;28r[1;1H[L[1;29r[1;1H[93m151 [m  for (a = 0; a < vocab_hash_size; a++) vocab_hash[a] = -1;[1;52H[?12l[?25h[2;24H[3;22H[?25l[4;34H[46m{[19;7H}[4;34H[?12l[?25h[?25l[m{[19;7H}[5;52H[?12l[?25h[?25l[37Cmm[6;1H[93m [m[7;52H[46m{[11;9H}[7;52H[?12l[?25h[?25l[m{[11;9H}[9;49H[?12l[?25h[10;52H[?25l[11;16H[46m{[18;9H}[11;16H[?12l[?25h[?25l[m{[18;9H}[10;52H[?12l[?25h[9;49H[?25l[7;52H[46m{[11;9H}[7;
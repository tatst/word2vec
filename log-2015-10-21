[93m168 [m  // Allocate memory for the binary tree construction
[93m169 [m  for (a = 0; a < vocab_size; a++) {
[93m170 [m    vocab[a].code = (char *)calloc(MAX_CODE_LENGTH, sizeof(char));[14;11H[?12l[?25hfre[?25le[46m([13C)[14;15H[?12l[?25h[?25l[m([13C)[14;16H[?12l[?25hvoca[?25lb[46m[[ma[46m][?12l[?25h[?25l[m[a][?12l[?25h[?25l[46m[[ma[46m][?12l[?25h[?25l[m[a][?12l[?25h.wor[?25l[14;15H[46m([13C)[?12l[?25h[?25l[m[14;15H([13C)[?12l[?25h; /* [?25l[29;1H[1m-- INSERT --[14;36H[?12l[?25h[?25l[m 1vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[14;36H[?12l[?25h[?25l12vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[14;37H[?12l[?25h[?25l25vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[14;38H[?12l[?25h[?25l5è¡Œç›®vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[14;42H[?12l[?25h[?25lã§vocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[14;44H[?12l[?25h[?25lç¢ºä¿ã—ãŸvocab[a].wordã®ãƒ¡ãƒ¢ãƒªã‚’è§£æ”¾ */[14;52H[?12l[?25h[29;1H[K[14;50H[?25l[?12l[?25h[?25l[29;1H/[?12l[?25hfree[?25l[1;28r[1;1H[13M[1;29r[16;1H[93m171 [m    vocab[a].point = (int *)calloc(MAX_CODE_LENGTH, sizeof(int));
[93m172 [m  }
[93m173 [m}
[93m174 
175 [m// Reduces the vocabulary by removing infrequent tokens
[93m176 [mvoid ReduceVocab() {
[93m177 [m  int a, b = 0;
[93m178 [m  unsigned int hash;
[93m179 [m  for (a = 0; a < vocab_size; a++) if (vocab[a].cn > min_reduce) {
[93m180 [m    vocab[b].cn = vocab[a].cn;
[93m181 [m    vocab[b].word = vocab[a].word;
[93m182 [m    b++;
[93m183 [m  } else free(vocab[a].word);[29;1H[K[28;14H[?12l[?25h[?25l
?free[1;11H[?12l[?25h[?25l[1;28r[1;1H[L[1;29r[1;1H[93m157 [m      vocab_size--; /* vocab_sizeã‹ã‚‰1å¼•ã */[29;1H[K[1;11H[?12l[?25h[?25l[1;28r[1;1H[2L[1;29r[1;1H[93m156 [m    if ((vocab[a].cn < min_count) && (a != 0)) { /* vocab[a].cnãŒmin_countæœªæº€ã‹ã¤aãŒ[2;1H[93m    [m0ã§ãªã„æ™‚ */[1;11H[?12l[?25h[?25l[1;28r[1;1H[2L[1;29r[1;1H[93m155 [m    // Words occuring less than min_count times will be discarded from the vocab /* mm[2;1H[93m    [min_countä»¥ä¸‹ã®é »åº¦ã®å˜èªã‚’vocabã‹ã‚‰é™¤å¤–ã™ã‚‹ */[1;11H[?12l[?25h[?25l[1;28r[1;1H[L[1;29r[1;1H[93m154 [m  for [46m([ma = 0; a < size; a++[46m)[m {[1;11H[?12l[?25h[?25l([20C)[2;11H[?12l[?25h[4;11H[6;11H[7;11H[8;11H[7;11H[6;11H[4;11H[?25l [46m([37C)[4;12H[?12l[?25h[?25l[m([46m([23C)[m[12C)[4;13H[?12l[?25h[?25l([23C)[4;14H[?12l[?25hvoca[?25lb[46m[[ma[46m][?12l[?25h[?25l[m[a][?12l[?25h[?25l[46m[[ma[46m][?12l[?25h[?25l[m[a][?12l[?25h.cn < min_coun[?25l[4;13H[46m([23C)[?12l[?25h[?25l[m[4;13H([23C)[?12l[?25h &&[?25l [46m([ma != 0[46m)[4;42H[?12l[?25h[?25l[m(a != 0)[4;43H[?12l[?25ha != [?25l[46m([ma != 0[46m)[?12l[?25h[?25l[4;12H([m[29C(a != 0)[46m)[?12l[?25h[?25l[m[4;12H([37C)[?12l[?25h[?25l [46m{[8;9H}[4;52H[?12l[?25h[?25l[m{[8;9H}[6;49H[?12l[?25h[7;52H[?25l[8;16H[46m{[15;9H}[8;16H[?12l[?25h[?25l[m{[15;9H}[9;52H[?12l[?25h[11;42H[12;52H[13;31H[14;37H[?25l[8;16H[46m{[15;9H}[?12l[?25h[?25l[1;34H{[m[8;16H{[15;9H}[16;7H[46m}[?12l[?25h[?25l[m[1;34H{[8;16H[46m{[15;9H}[m[16;7H}[15;9H[?12l[?25h[?25l[8;16H{[15;9H}[14;37H[?12l[?25h[13;31H[12;52H[11;42H[9;52H[?25l[8;16H[46m{[15;9H}[8;16H[?12l[?25h[?25l[m{[15;9H}[7;52H[?12l[?25h[6;49H[?25l[4;52H[46m{[8;9H}[4;52H[?12l[?25h[?25l[m{[8;9H}[2;52H[?12l[?25h[?25l[1;34H[46m{[16;7H}[1;34H[?12l[?25h[?25l[m{[16;7H}[2;52H[?12l[?25h
[93m234 [m    [46m}[m
[93m235 [m    count[vocab_size + a] = count[min1i] + count[min2i]; /* count[vocab_size[24;1H[94m@                                                                               [23;9H[?12l[?25h[?25l[1;24r[m[24;1H

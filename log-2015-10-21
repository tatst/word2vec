[93m235 [m    count[vocab_size + a] = count[min1i] + count[min2i]; /* count[vocab_sizee[24;1H[93m    [m + a]ã«count[min1i] + count[min2i]ã‚’ä»£å…¥ */[23;9H[?12l[?25h[?25l[1;24r[1;1H[2M[1;25r[23;1H[93m236 [m    parent_node[min1i] = vocab_size + a; /* min1iã®è¦ªãƒãƒ¼ãƒ‰ã«vocab_size + a  [24;1H[93m    [mã‚’ä»£å…¥ */[23;9H[?12l[?25h[?25l[1;24r[1;1H[2M[1;25r[23;1H[93m237 [m    parent_node[min2i] = vocab_size + a; /* min2iã®è¦ªãƒãƒ¼ãƒ‰ã«vocab_size + a  [24;1H[93m    [mã‚’ä»£å…¥ */[23;9H[?12l[?25h[?25l[1;24r[24;1H
